{
  "name": "kizuna-sdk",
  "version": "1.0.0",
  "description": "Blockchain connectivity for ClawBot - Trade NFTs, manage wallets, AI trading",
  "author": "KizunaSDK Team",
  "homepage": "https://github.com/your-repo/kizuna-sdk",
  "platform": ["clawbot", "openclaw"],
  "commands": {
    "wallet": {
      "description": "Get wallet address and balance",
      "usage": ["wallet", "balance", "my address"]
    },
    "transfer": {
      "description": "Transfer tokens or NFTs",
      "usage": ["transfer <amount> <token> to <address>"]
    },
    "nft": {
      "description": "Get NFT floor prices and manage collection",
      "usage": ["nft floor <collection>", "nft price <collection>"]
    },
    "trade": {
      "description": "AI-powered trading decisions",
      "usage": ["should I buy <collection>", "analyze <collection>"]
    },
    "alert": {
      "description": "Set price alerts for NFT collections",
      "usage": ["alert when <collection> above <price>"]
    },
    "swap": {
      "description": "Swap tokens",
      "usage": ["swap <amount> <from> to <to>"]
    }
  },
  "env": {
    "required": ["PRIVATE_KEY", "GASLESS_API_KEY"],
    "optional": ["GEMINI_API_KEY", "RPC_URL", "CHAIN_ID"]
  },
  "example": {
    "env": "PRIVATE_KEY=0x... GASLESS_API_KEY=...",
    "npm_install": "npm install kizuna-sdk"
  }
}
